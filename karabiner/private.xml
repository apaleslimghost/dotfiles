  <item>
    <name>Command → Command/Escape</name>
    <appendix>Press Command for Escape, hold it for Command</appendix>
    <identifier>private.commandescape</identifier>
    <autogen>
      --KeyOverlaidModifier--
      KeyCode::COMMAND_L,
      KeyCode::COMMAND_L,
      KeyCode::ESCAPE
    </autogen>
  </item>
  <item>
    <name>Backtick ←→ Backslash</name>
    <appendix>Swap backtick and backtick on the WASD v2</appendix>
    <identifier>private.backtickslash</identifier>

    <device_only>DeviceVendor::GENERIC, DeviceProduct::WASD_V2</device_only>
    <autogen>__KeyToKey__ KeyCode::DANISH_DOLLAR, KeyCode::BACKQUOTE</autogen>
    <autogen>__KeyToKey__ KeyCode::BACKQUOTE, KeyCode::DANISH_DOLLAR</autogen>
  </item>

